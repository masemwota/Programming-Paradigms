object functional {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  
	  /**
	  * PROBLEM 1
	  */
		def compose(f: Int => Int, g: Int =>Int): Int=>Int =
		{
		   def r(x: Int): Int = f(g(x));
		   r _
		}                                 //> compose: (f: Int => Int, g: Int => Int)Int => Int
}